<h1>My Teams</h1>

{{#each teams}}
  <div class="team">
    <h2>{{team_name}}</h2>
    <h3>Members:</h3>
    <ul>
      {{#each members}}
        <li><strong>{{name}}</strong> ({{username}}) - {{email}} - Role: {{role}}</li>
      {{/each}}
    </ul>
  </div>
{{/each}}

<div class="footer">
  <a href="/manage-team">Add/Remove team members here</a>
</div>

{{> footer}}